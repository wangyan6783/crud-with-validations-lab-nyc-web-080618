<h1>Songs#new</h1>
<p>Find me in app/views/songs/new.html.erb</p>

<%= form_for @song do |f| %>
  <%= f.label "Title" %>
  <%= f.text_field :title %>
  <%= f.label "Released" %>
  <%= f.check_box :released %>
  <%= f.label "Release Year" %>
  <%= f.number_field :release_year %>
  <%= f.label "Artist Name" %>
  <%= f.text_field :artist_name %>
  <%= f.label "Genre" %>
  <%= f.text_field :genre %>
  <%= f.submit %>
<% end %>
